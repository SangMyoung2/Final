<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.boot.mapper.MapMapper">


<select id="getLists" parameterType="hashMap" resultType="com.spring.boot.dto.MapDTO">
	select * from (
	select rownum rnum, data.* from (
	select lat,lng,id from latlng
	order by id asc) data)
</select>

<select id="getData" parameterType="hashMap" resultType="com.spring.boot.dto.TestBoardDTO">
	select * from meet
</select>

<select id="getOneData" parameterType="int" resultType="com.spring.boot.dto.TestBoardDTO">
	select * from meet where listNum=#{listNum}
</select>

<select id="getTitleData" parameterType="String" resultType="com.spring.boot.dto.TestBoardDTO">
	select * from meet where title like '%' || #{title} || '%'
</select>

<select id="getOneLocation" parameterType="int" resultType="com.spring.boot.dto.MapDTO">
	select * from latlng where id=#{id}
</select>

</mapper>